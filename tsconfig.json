{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "@Components/*": [
        "src/Components/*"
      ],
      "@Redux/*": [
        "src/Redux/*"
      ],
      "@Assets/*": [
        "src/Assets/*"
      ],
      "@Config/*": [
        "src/Config/*"
      ],
      "@HOC": [
        "src/HOC/*"
      ],
      "@Layout/*": [
        "src/Layout/*"
      ],
      "@Routers/*": [
        "src/Routers/*"
      ],
      "@Services/*": [
        "src/Services/*"
      ],
      "@Shared/*": [
        "src/Shared/*"
      ],
      "@Views/*": [
        "src/Views/*"
      ],
    },
  },
  "include": [
    "src",
    "craco.config.js",
    "public/Components",
    "tailwind.config.cjs"
  ]
}